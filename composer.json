{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_theme": "1.x-dev@dev",
        "drupal/admin_toolbar": "^3.2",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/blockgroup": "^2.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/core-composer-scaffold": "9.5.9",
        "drupal/core-project-message": "9.5.9",
        "drupal/core-recommended": "10.1.6",
        "drupal/country_path": "^1.5",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/domain": "^2.0@beta",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/image_effects": "^3.4",
        "drupal/image_style_quality": "^1.4",
        "drupal/linkit": "^6.1",
        "drupal/login_redirect_per_role": "^1.9",
        "drupal/lupus_taxonomy_import": "^1.2",
        "drupal/menu_trail_by_path": "^2.0",
        "drupal/metatag": "^1.22",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/rebuild_cache_access": "^1.8",
        "drupal/scss_compiler": "^1.1",
        "drupal/search_api": "^1.28",
        "drupal/slick_lightbox": "^1.5",
        "drupal/smart_trim": "^2.0",
        "drupal/tvi": "^2.0@alpha",
        "drupal/twig_tweak": "^3.2",
        "drupal/upgrade_status": "^4.0",
        "drupal/views_bootstrap": "^5.5@alpha",
        "drupal/views_tree": "^2.0@RC",
        "drupal/webform": "^6.2@beta",
        "drush/drush": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
